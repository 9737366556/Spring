<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-4.2.xsd">


	<bean id="beanFactoryConstructorDemo"
		class="com.nikunj.springcore.beanFactoryDemo.demo.BeanFactoryConstructorDemo">
		<constructor-arg value="10" type="int" />
		<constructor-arg value="nikunj" />
	</bean>

	<bean id="beanFactorySetterDemo"
		class="com.nikunj.springcore.beanFactoryDemo.demo.BeanFactorySetterDemo">
		<property name="id" value="1" />
		<property name="name" value="kunj" />
	</bean>

</beans>